<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Name="DictionaryColors">
    <Color x:Key="MainBackground">#FFFFFF</Color>
    <Color x:Key="SecondBackground">#7FFF00</Color>
    <Color x:Key="Accent">#00FA9A</Color>
    <Color x:Key="BigDiscount">#2E8B57</Color>
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="{StaticResource MainBackground}"/>
    <SolidColorBrush x:Key="SecondBackgroundBrush" Color="{StaticResource SecondBackground}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}"/>
    <SolidColorBrush x:Key="BigDiscountBrush" Color="{StaticResource BigDiscount}"/>
</ResourceDictionary>